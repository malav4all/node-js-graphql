{
  "name": "graphql-mongodb-server",
  "version": "1.0.0",
  "description": "A GraphQL MongoDB server.",
  "main": "index.js",
  "scripts": {
    "start": "npm run server",
    "server": "nodemon --exec babel-node server/index.js"
  },
  "keywords": [
    "graphql",
    "mongodb",
    "boilerplate",
    "server"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  },
  "dependencies": {
    "@graphql-tools/merge": "^6.2.14",
    "concurrently": "^4.1.0",
    "dotenv": "^6.1.0",
    "graphql": "^14.0.2",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "^1.14.10",
    "mongoose": "^5.3.13",
    "nodemon": "^1.18.10",
    "uuid": "^3.3.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/tests/jest.config"
    ],
    "transform": {
